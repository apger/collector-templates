      "collectRequestHeaders": [
        {
          "name": "Content-Type",
          "value": "REDACTED"
        },
        {
          "name": "Authorization",
          "value": "REDACTED"
        }
      ],
      "collectUrl": "'http://35.91.90.120:8080/api/v2/auditevents'",
      "collectRequestParams": [
        {
          "name": "limit",
          "value": "'5'"
        },
        {
          "name": "start_time",
          "value": "'2025-07-14T16:32:50-03:00'"
        }
      ],
      "collectBody": "{\n    \"limit\": (cursor === NULL) ? 50 : NULL, \n    \"start_time\": (cursor === NULL) ? `${C.Time.strftime(state.latestTime || earliest, '%Y-%m-%dT%H:%M:%S-00:00')}` : NULL,\n    \"end_time\": (cursor === NULL) ? `${C.Time.strftime(latest || Date.now()/1000, '%Y-%m-%dT%H:%M:%S-00:00')}` : NULL,\n    \"cursor\": cursor || NULL\n}"
    },
    "destructive": false,
    "encoding": "utf8",
    "type": "rest"
  },
  "input": {
    "type": "collection",
    "staleChannelFlushMs": 10000,
    "sendToRoutes": true,
